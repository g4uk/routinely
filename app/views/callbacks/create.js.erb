<% if @callback.persisted? %>
  $(".<%= @callback.class.name.underscore.dasherize %>-form[data-target-id='<%= @callback.target.to_global_id %>']").replaceWith("<%= j render('callback', { callback: @callback }) %>");
<% else %>
  $(".<%= @callback.class.name.underscore.dasherize %>-form[data-target-id='<%= @callback.target.to_global_id %>']").replaceWith("<%= j render('actor', { actor: @callback.target }) %>");
<% end %>
